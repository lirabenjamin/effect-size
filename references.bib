  author = {Moshe, Isaac and Terhorst, Yannik and Philippi, Paula and Domhardt, Matthias and Cuijpers, Pim and Cristea, Ioana and {Pulkki-R{\aa}back}, Laura and Baumeister, Harald and Sander, Lasse B.},
