@article{kornerDominancePrestigeMetaanalytic2022,
  title = {Dominance and Prestige: {{Meta-analytic}} Review of Experimentally Induced Body Position Effects on Behavioral, Self-Report, and Physiological Dependent Variables.},
  author = {K{\"o}rner, Robert and R{\"o}seler, Lukas and Sch{\"u}tz, Astrid and Bushman, Brad J.},
  year = {2022},
  journal = {Psychological Bulletin},
  volume = {148},
  number = {1-2},
  pages = {67--85},
  publisher = {{American Psychological Association}},
  address = {{US}},
  issn = {1939-1455(Electronic),0033-2909(Print)},
  doi = {10.1037/bul0000356},
  abstract = {Early research on body positions suggested that engaging in certain nonverbal displays can lead to changes in self-report, behavioral, and physiological dependent variables. Still, there has been intense criticism regarding the replicability of these effects. To determine what effects are valid, we conducted a meta-analytic review on body position studies. We used the dominance\textendash prestige framework and distinguished between high-power poses representing dominance and upright postures representing prestige. We preregistered our meta-analysis, used the largest sample of studies thus far, and analyzed several theoretical and exploratory moderator variables. Based on 313 effects from 88 studies involving 9,779 participants, evidence was obtained for an overall statistically significant effect of body positions that was not trivial in size, g = 0.35 (95\% CI [0.28,0.42]). Both the poses and postures showed effects for self-report and behavioral dependent variables but not for physiological dependent variables. However, sensitivity analyses suggested that effects for behavioral dependent variables were influenced by publication bias and/or outliers. Effects were noticeably larger in studies without cover stories and in studies that used within-subjects designs, suggesting that demand characteristics might partially explain the results. Whether participants were male or female, students or nonstudents, or from an individualistic or collectivistic culture did not make a difference. We also present an app that researchers can use to enter data from future studies and thus obtain up-to-date meta-analytical results on this topic. Future research should investigate whether high-power poses/upright postures increase effects and/or whether low-power poses/slumped postures decrease effects. (PsycInfo Database Record (c) 2022 APA, all rights reserved)},
  keywords = {*Body Language,*Dependent Variables,*Dominance,*Interpersonal Control,*Posture,Body Size,Nonverbal Ability,Self-Report}
}

