@article{anglimPersonalityIntelligenceMetaanalysis2022,
  title = {Personality and Intelligence: {{A}} Meta-Analysis.},
  author = {Anglim, Jeromy and Dunlop, Patrick D. and Wee, Serena and Horwood, Sharon and Wood, Joshua K. and Marty, Andrew},
  year = {2022},
  journal = {Psychological Bulletin},
  volume = {148},
  number = {5-6},
  pages = {301--336},
  publisher = {{American Psychological Association}},
  address = {{US}},
  issn = {1939-1455(Electronic),0033-2909(Print)},
  doi = {10.1037/bul0000373},
  abstract = {This study provides a comprehensive assessment of the associations of personality and intelligence. It presents a meta-analysis (N = 162,636, k = 272) of domain, facet, and item-level correlations between personality and intelligence (general, fluid, and crystallized) for the major Big Five and HEXACO hierarchical frameworks of personality: NEO Personality Inventory\textendash Revised, Big Five Aspect Scales, Big Five Inventory\textendash 2, and HEXACO Personality Inventory\textendash Revised. It provides the first meta-analysis of personality and intelligence to comprehensively examine (a) facet-level correlations for these hierarchical frameworks of personality, (b) item-level correlations, (c) domain- and facet-level predictive models. Age and sex differences in personality and intelligence, and study-level moderators, are also examined. The study was complemented by four of our own unpublished data sets (N = 26,813) which were used to assess the ability of item-level models to provide generalizable prediction. Results showed that openness ({$\rho$} = .20) and neuroticism ({$\rho$} = -.09) were the strongest Big Five correlates of intelligence and that openness correlated more with crystallized than fluid intelligence. At the facet level, traits related to intellectual engagement and unconventionality were more strongly related to intelligence than other openness facets, and sociability and orderliness were negatively correlated with intelligence. Facets of gregariousness and excitement seeking had stronger negative correlations, and openness to aesthetics, feelings, and values had stronger positive correlations with crystallized than fluid intelligence. Facets explained more than twice the variance of domains. Overall, the results provide the most nuanced and robust evidence to date of the relationship between personality and intelligence. (PsycInfo Database Record (c) 2023 APA, all rights reserved)},
  keywords = {*Cognitive Ability,*Five Factor Personality Model,*Intelligence,*Personality,*Personality Traits,Fluid Intelligence,Openness to Experience,Personality Measures},
  file = {/Users/blira/Zotero/storage/9Z6T6U9R/Anglim et al. - 2022 - Personality and intelligence A meta-analysis..pdf}
}

