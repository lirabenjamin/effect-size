@article{cahillFamilismValuesAdjustment2021,
  title = {Familism Values and Adjustment among {{Hispanic}}/{{Latino}} Individuals: {{A}} Systematic Review and Meta-Analysis.},
  author = {Cahill, Karina M. and Updegraff, Kimberly A. and Causadias, Jos{\'e} M. and Korous, Kevin M.},
