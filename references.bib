@article{bleidornPersonalityStabilityChange2022a,
  title = {Personality Stability and Change: {{A}} Meta-Analysis of Longitudinal Studies.},
  author = {Bleidorn, Wiebke and Schwaba, Ted and Zheng, Anqing and Hopwood, Christopher J. and Sosa, Susana S. and Roberts, Brent W. and Briley, D. A.},
