@article{liTraitStateAffective2022,
  title = {Trait and State Affective Experience among High-Risk People in the Schizophrenia Spectrum: {{A}} Meta-Analytic Review.},
  author = {Li, Lilian Y. and Dent, Amy L. and Donberg, Ryan and Martin, Elizabeth A.},
  year = {2022},
