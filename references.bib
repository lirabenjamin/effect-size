@article{imutaMetaanalyticReviewSocial2022,
  title = {A Meta-Analytic Review on the Social\textendash Emotional Intelligence Correlates of the Six Bullying Roles: {{Bullies}}, Followers, Victims, Bully-Victims, Defenders, and Outsiders.},
  author = {Imuta, Kana and Song, Sumin and Henry, Julie D. and Ruffman, Ted and Peterson, Candida and Slaughter, Virginia},
  year = {2022},
  journal = {Psychological Bulletin},
  volume = {148},
  number = {3-4},
  pages = {199--226},
  publisher = {{American Psychological Association}},
  address = {{US}},
  issn = {1939-1455(Electronic),0033-2909(Print)},
  doi = {10.1037/bul0000364},
  abstract = {Bullying is one of the most common peer-relational problems experienced by children and adolescents worldwide. One reason bullying is so widespread is that it is a dynamic, group process that involves multiple roles\textemdash namely, bullies, followers, victims, bully-victims, defenders, and outsiders. Given the profoundly negative impact of exposure to bullying on well-being across development, researchers have sought to identify the social\textendash emotional intelligence profiles of the different bullying roles to develop effective interventions. The present meta-analysis integrated findings from 128 studies involving 187,454 children and adolescents between 3 and 18 years of age to investigate how four interrelated but distinct facets of social\textendash emotional intelligence\textemdash affective empathy, cognitive empathy, affective theory of mind (ToM), and cognitive ToM\textemdash relate to different bullying roles. Significant associations were found for bullies, followers, and defenders, but not for being the victim, bully-victim, or outsider. For bullies and followers, we found negative relations with both affective and cognitive empathy (rs = -.11 to -.22), but no relation with either type of ToM. For defending, we found positive relations with all four facets of social\textendash emotional intelligence (rs = .18\textendash.32). These findings suggest that a successful antibullying program may entail a combination of motivating children and adolescents with bullying tendencies to care about others' feelings, and empowering their classmates to become strong perspective-takers who can stand up for those in need of help. (PsycInfo Database Record (c) 2022 APA, all rights reserved)},
  keywords = {*Bullying,*Emotional Intelligence,*Empathy,*Socioemotional Functioning,*Victimization,Peer Relations,Theory of Mind}
}

