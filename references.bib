@article{chuaSuperiorityExternalAttentional2021,
  title = {Superiority of External Attentional Focus for Motor Performance and Learning: {{Systematic}} Reviews and Meta-Analyses.},
  author = {Chua, Lee-Kuen and {Jimenez-Diaz}, Judith and Lewthwaite, Rebecca and Kim, Taewon and Wulf, Gabriele},
  year = {2021},
