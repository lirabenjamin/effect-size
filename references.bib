@article{bairdUnderstandingEffectsTime2021,
  title = {Understanding the Effects of Time Perspective: {{A}} Meta-Analysis Testing a Self-Regulatory Framework.},
  author = {Baird, Harriet M. and Webb, Thomas L. and Sirois, Fuschia M. and {Gibson-Miller}, Jilly},
