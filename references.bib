@article{bueckerLonelinessEmergingAdults2021,
  title = {Is Loneliness in Emerging Adults Increasing over Time? {{A}} Preregistered Cross-Temporal Meta-Analysis and Systematic Review.},
  author = {Buecker, Susanne and Mund, Marcus and Chwastek, Sandy and Sostmann, Melina and Luhmann, Maike},
