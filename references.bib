@article{morgenrothWhoWhenWhy2020,
  title = {The Who, When, and Why of the Glass Cliff Phenomenon: {{A}} Meta-Analysis of Appointments to Precarious Leadership Positions.},
  author = {Morgenroth, Thekla and Kirby, Teri A. and Ryan, Michelle K. and Sudk{\"a}mper, Antonia},
