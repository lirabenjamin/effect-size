@article{curranYoungPeoplePerceptions2022,
  title = {Young People's Perceptions of Their Parents' Expectations and Criticism Are Increasing over Time: {{Implications}} for Perfectionism.},
  author = {Curran, Thomas and Hill, Andrew P.},
