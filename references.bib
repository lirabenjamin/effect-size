@article{hoemannExpertiseEmotionScoping2021,
  title = {Expertise in Emotion: {{A}} Scoping Review and Unifying Framework for Individual Differences in the Mental Representation of Emotional Experience.},
  author = {Hoemann, Katie and Nielson, Catie and Yuen, Ashley and Gurera, J. W. and Quigley, Karen S. and Barrett, Lisa Feldman},
