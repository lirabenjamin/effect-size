@article{mckayVisualAttentionalOrienting2021,
  title = {Visual Attentional Orienting by Eye Gaze: {{A}} Meta-Analytic Review of the Gaze-Cueing Effect.},
  author = {McKay, Kate T. and Grainger, Sarah A. and Coundouris, Sarah P. and Skorich, Daniel P. and Phillips, Louise H. and Henry, Julie D.},
  year = {2021},
  journal = {Psychological Bulletin},
  volume = {147},
